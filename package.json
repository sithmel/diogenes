{
  "name": "diogenes",
  "version": "0.5.2",
  "description": "Defines and executes functions with a common interface (services) configured in a directed acyclic graph",
  "main": "src/diogenes.js",
  "scripts": {
    "test": "mocha tests/*.js",
    "watch": "npm run test -- -w",
    "lint": "./node_modules/.bin/eslint --ext .js ./src ./tests",
    "release:major": "./node_modules/.bin/npm-release major",
    "release:minor": "./node_modules/.bin/npm-release minor",
    "release:patch": "./node_modules/.bin/npm-release patch",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "keywords": [
    "plugins",
    "dependency injection",
    "services"
  ],
  "repository": "git@github.com:sithmel/diogenes.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^1.10.0",
    "eslint": "^1.10.3",
    "husky": "^0.10.2",
    "mocha": "^2.1.0",
    "npm-release": "^1.0.0"
  },
  "dependencies": {
    "occamsrazor": "^4.4.0",
    "setimmediate": "^1.0.4"
  }
}
